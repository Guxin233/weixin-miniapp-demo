<!-- 事项内容 -->
<view class='content'>{{content}}</view>

<!-- 环形时间进度条 -->
<div class="circleProgress_wrapper">
  <div class="wrapper right">
    <div class="circleProgress rightcircle{{minute}}"></div>
  </div>
  <div class="wrapper left">
    <div class="circleProgress leftcircle{{minute}}"></div>
  </div>
</div>

<!-- 选择时间 -->
<view class='timeView'>
	<block class='timeSelector'>5</block>
	<block class='timeSelector'>15</block>
	<block class='timeSelector'>52</block>
</view>